@model TARpe22ShopRohusaar.Models.Weather.WeatherViewModelOW

<h1>City</h1>

<div class="container">
    <div class="form-group col-md-3">
        <h2>Forecast for the selected City</h2>
        <dl class="row">
            <div class="form-group-row">
                <label asp-for="All" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.All)
            </div>
            <div class="form-group-row">
                <label asp-for="Lon" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Lon)
            </div>
            <div class="form-group-row">
                <label asp-for="Lat" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Lat)
            </div>
            <div class="form-group-row">
                <label asp-for="Temp" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Temp)
            </div>
            <div class="form-group-row">
                <label asp-for="FeelsLike" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.FeelsLike)
            </div>
            <div class="form-group-row">
                <label asp-for="TempMin" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.TempMin)
            </div>
            <div class="form-group-row">
                <label asp-for="TempMax" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.TempMax)
            </div>
            <div class="form-group-row">
                <label asp-for="Pressure" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Pressure)
            </div>
            <div class="form-group-row">
                <label asp-for="Humidity" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Humidity)
            </div>
            <div class="form-group-row">
                <label asp-for="SeaLevel" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.SeaLevel)
            </div>
            <div class="form-group-row">
                <label asp-for="GrndLevel" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.GrndLevel)
            </div>
            <div class="form-group-row">
                <label asp-for="_1h" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model._1h)
            </div>
            <div class="form-group-row">
                <label asp-for="Main" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Main)
            </div>
            <div class="form-group-row">
                <label asp-for="Visibility" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Visibility)
            </div>
            <div class="form-group-row">
                <label asp-for="Name" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Name)
            </div>
            <div class="form-group-row">
                <label asp-for="Country" class="col-sm-7 col-form-label"></label>
                @Html.DisplayFor(model => model.Country)
            </div>
        </dl>
    </div>
</div>